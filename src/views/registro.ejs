<div class="container ">
  <h3>Register</h3>
  <div class="row">
    <!--FORM-->
    <div class="col-md-9">
      <div class="card">
        <div class="card-body">
          <form action="signup" method="post">
            <div class="col-md-6">
              <div class="form-group form">
                <label for="Name">Name</label>
                <input class="form-control" type="text" name="name" placeholder="Name" required=true>
              </div>
              <div class="form-group">
                <label for="Name">Documento</label>
                <input class="form-control" type="number" name="doc" placeholder="Document" required=true>
              </div>
              <div class="form-group">
                <label for="Name">Email</label>
                <input class="form-control" type="email" name="email" placeholder="Mail" required=true>
              </div>
              <div class="form-group">
                <label for="Name">Phone</label>
                <input class="form-control" type="number" name="tel1" placeholder="Tel Mobile" required=true
                  maxlength="12">
              </div>

            </div>
            <div class="col-md-6">
              <div class="form-group">
                <label for="Name">Last Name</label>
                <input class="form-control" type="text" name="last_name" placeholder="Last Name" required=true>
              </div>
              <div class="form-group">
                <label for="Name">Password</label>
                <input class="form-control" type="password" name="password" placeholder="Password" required=true
                  minlength="6">
              </div>
              <div class="form-group">
                <label for="Name">Other Email</label>
                <input class="form-control" type="email" name="email2" placeholder="Mail Aux.">
              </div>
              <div class="form-group">
                <label for="Name">Other Phone</label>
                <input class="form-control" type="number" name="tel2" placeholder="Other Phone" maxlength="12">
              </div>
            </div>
        

      <div class="form-group" style="width: 10%; float:left; margin: 13px;">
        <button class="btn btn-primary " type="submit">
          Crear contacto
        </button>
        <div>
          <%if (signinMessage) {%>
          <p><%= signinMessage %></p>
          <% } %>
          <%if (signupMessage) {%>
          <p><%= signupMessage %></p>
          <% } %>
        </div>
        <div>
          </form>
        </div>
      </div>
    </div>
  </div>
</form>
</div>
   